{
  "name": "jiangnan-travel-blog",
  "version": "1.0.0",
  "description": "Discover the poetic water towns of Southern China",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "lint": "eslint . --ext .js,.html --fix",
    "format": "prettier --write .",
    "optimize-images": "node scripts/optimize-images.js",
    "generate-sw": "node scripts/generate-sw.js",
    "deploy": "npm run build && npm run generate-sw",
    "build:analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf dist",
    "setup": "node scripts/install-dependencies.js",
    "db:migrate": "node scripts/migrate-articles.js",
    "db:seed": "node scripts/seed-database.js",
    "start:api": "node api/server.js",
    "start": "node server.js"
  },
  "keywords": [
    "travel",
    "china",
    "jiangnan",
    "water-towns",
    "tourism",
    "blog"
  ],
  "author": "Jiangnan Travel Blog",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.55.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "sharp": "^0.33.0",
    "terser": "^5.24.0",
    "vite": "^5.0.0",
    "workbox-build": "^7.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dompurify": "^3.2.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "marked": "^9.1.6",
    "workbox-window": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liyuan210/traverltip.git"
  },
  "bugs": {
    "url": "https://github.com/liyuan210/traverltip/issues"
  },
  "homepage": "https://github.com/liyuan210/traverltip#readme"
}
